@model IEnumerable<Project.Models.ThesaurusItem>

@{
    ViewBag.Title = "Search";
}

<h2>Results for @ViewData["Word"]</h2>

<p>
    
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Word)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Word)
        </td>
    </tr>
}

</table>
